<?xml version="1.0" encoding="UTF-8"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
	<% (0..@filecount - 1).each do |i| %>
		<sitemap>
			<loc><%= @base_url %>/tirelistings<%= i.to_s %>.xml</loc>
			<lastmod><%= Time.now.to_s(:w3c) %></lastmod>
		</sitemap>
	<% end %>
</sitemapindex>