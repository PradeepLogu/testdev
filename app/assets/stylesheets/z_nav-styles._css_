/* CSS Document */

/* ======= HEADER OBJECTS AND NAVIGATION ========= */
#header-container{width: 100%; height: 60px; background: url(header.png) top repeat-x; position:fixed; z-index:5000;}
#header-objects{width: 980px; margin: 0px auto 0px 0px;}
#header-nav{width: 620px; margin: 0px 10px; float: right; text-align: right;}
#header-logo{width: 300px; margin: 3px 10px 0px; float: left;}
ul.headernav{list-style-type: none; display: block; height: 49px; clear: right;}
.headernav li{float: right; height: 49px; width: 100px; background: #878787 url(header-button.png) top repeat-x; display: block;}
li a, li a:link, li a:visited{width: 100px !important; height: 20px; padding: 10px 0px 19px 0px; font-family: Impact, "Lucida Sans Unicode", "Lucida Grande", sans-serif; font-size: 20px; color: #DDD; text-shadow: 0px -1px 1px #333; text-decoration: none; text-align: center; border-right: 1px solid #556; border-left: 1px solid #111; display: block;}
li a:focus, li a:hover, li.urhere{background: #282828 url(header-button.png) bottom repeat-x; color: #F63; text-shadow: 1px 1px 2px #222; display: block;}

.dropdown-menu
{
	min-width: 0px;
	background-color: transparent;
	padding: 0px 0;
}
.modal-backdrop {
  position: fixed;
  top: 200;
  right: 200;
  bottom: 200;
  left: 200;
  z-index: 1040;
}

.modal-backdrop.fade {
  opacity: 0;
}

.modal-backdrop, .modal-backdrop.fade.in {
  opacity: 0.8;
  filter: alpha(opacity=80);
}

.modal, .modal-footer {background-color: darkgray;}

.popover-content, .popover-title {background-color: darkgray;}
